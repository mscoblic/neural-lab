{
  "data_config": {
    "excel_path": "data/brachi.xlsx",
    "schema": {
      "header": null,
      "input_cols": [
        0,
        1,
        2,
        3
      ],
      "output_slices": [
        [
          4,
          28
        ],
        [
          28,
          52
        ]
      ],
      "infer_T_from_outputs": true
    },
    "normalization": {
      "inputs": "standardize",
      "outputs": "standardize"
    },
    "task_name": "brachi"
  },
  "model_config": {
    "kind": "ffn",
    "ffn": {
      "hidden_dim": 128,
      "n_hidden": 3,
      "activation": "relu",
      "dropout": 0.0
    }
  },
  "run_config": {
    "seed": 42,
    "device": "auto",
    "epochs": 100,
    "batch_size": 64,
    "lr": 0.001,
    "val_split": 0.1,
    "test_ratio": 0.1,
    "num_workers": 0,
    "early_stopping": {
      "enabled": true,
      "patience": 10
    },
    "logging": {
      "run_name": "",
      "out_dir": "runs",
      "save_every": 0
    }
  }
}